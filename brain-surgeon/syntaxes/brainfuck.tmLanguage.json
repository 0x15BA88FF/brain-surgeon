{
    "scopeName": "source.brainfuck",
    "name": "Brainfuck",
    "fileTypes": [
        "bf",
        "brainfuck"
    ],
    "patterns": [
        {
            "match": "[><]",
            "name": "keyword.operator.pointer.brainfuck"
        },
        {
            "match": "[+\\-]",
            "name": "keyword.operator.arithmetic.brainfuck"
        },
        {
            "match": "[\\[\\]]",
            "name": "keyword.control.loop.brainfuck"
        },
        {
            "match": "[.,]",
            "name": "keyword.operator.io.brainfuck"
        },
        {
            "match": "[^><+\\-\\[\\].,\\s]",
            "name": "comment.ignored.brainfuck"
        }
    ],
    "repository": {}
}